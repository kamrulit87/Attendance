                <!-- Start Header Navigation -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-menu">
                        <i class="fa fa-bars"></i>
                    </button>
                    <a class="navbar-brand" href="@Url.Action("Index", "Home", new { area=""})">
                        <img src="@Url.Content("~/Content/built_in/assets/img/invogue.png")" alt="logo" class="brand" data-src="@Url.Content("~/Content/built_in/assets/img/invogue.png")" data-src-retina="@Url.Content("~/Content/built_in/assets/img/invogue.png")">
                    </a>
                </div>
                <!-- End Header Navigation -->
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="navbar-menu">
                    <ul class="nav navbar-nav navbar-right" data-in="fadeInDown" data-out="fadeOutUp">

                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">Reports <span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li>@Html.ActionLink("Employee In-Out", "Index", "EmpInOut")</li>
                                <li>@Html.ActionLink("Add Comments", "Index", "AddComments")</li>
                                <li>@Html.ActionLink("Employee inside Hour", "Index", "EmpInHr")</li>
                                <li>@Html.ActionLink("Employee Outside Hour", "Index", "AllEmpOutsideHour")</li>
                                <li>@Html.ActionLink("Employee In", "Index", "AllEmpIn")</li>
                                <li>@Html.ActionLink("Employee Out", "Index", "AllEmpOut")</li>

                            </ul>
                        </li>
                    </ul>
                </div><!-- /.navbar-collapse -->
            </nav>
            <div class="clock-content">
                
                <div id="info-box">
                    <div id="clock"></div>

                </div>
            </div>
            <div class="right-heading">
                <h3 style="color:white;font-weight:bold;"> <span style="color:red"> BENGAL WELLBEING </span> ATTENDANCE </h3>
                
            </div>
        </div>

        <!-- Start Body -->
        <div class="page-content-wrapper ">
            <div class="content ">
                <div id="loading">
                    <img id="imagen" src="~/Content/built_in/pages/img/progress/progress-circle-success.svg" alt="Loading..." style="left: 50%;" />
                    <div id="timer">Timer</div>
                </div>
                @RenderBody()

            </div>

            <div class="container-fluid container-fixed-lg footer">
                <div class="copyright sm-text-center">
                    <p class="small no-margin pull-left sm-pull-reset">
                        <span class="hint-text">Copyright &copy;  2018 </span>
                        <span class="font-montserrat">Invogue Software Limited</span>.
                        <span class="hint-text">All rights reserved. </span>
                        <span class="sm-block"><a href="#" class="m-l-10 m-r-10">Terms of use</a> | <a href="#" class="m-l-10">Privacy Policy</a></span>
                    </p>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
        <!-- Start Body -->
    </div>


    <script>
        $(document).ready(function ($) {
            // Hide Loader
            $("#loading").hide();
            //$("#btnPre").click(function () {
            //    $('#loading').show();
            //})
            // menu
            var device_width = $(window).width();
            if (device_width < 1000) {
                $("body").removeClass('windows');
                $("body").removeClass('desktop');
                $("body").removeClass('sidebar-visible');
                $("body").removeClass('menu-pin');
            }

            //menu selected
            $(".menu-items a").each(function () {
                var curURL = window.location.href; // Current full URL
                curURL = curURL.replace(/\/$/, "");
                curURL = decodeURIComponent(curURL);

                var originUrl = window.location.origin; // Domain Part from curURL
                var newUrl = curURL.replace(originUrl, '');// Remove Domain Part

                var href = $(this).attr('href');

                if (newUrl === href) {
                    $(this).closest('li').addClass('open active');
                    $(this).closest('li').parents("li").addClass('open active');
                    $(this).closest('li').parents("ul").css({ "display": "block" });
                }
                else {
                    var secondurl = $("ul.breadcrumb a:eq(1)").attr("href");
                    if (secondurl === href) {
                        $(this).closest('li').addClass('open active');
                        $(this).closest('li').parents("li").addClass('open active');
                        $(this).closest('li').parents("ul").css({ "display": "block" });
                    }
                }
            });
        });

        $(document).ready(function () {
            $(window).on('load', function () {
                $("#loading").hide();
            });
            $("#attendance").click(function () {
                //$("#loading").show();
                $.ajax({
                    url: 'Home/DownloadFromDevice',
                    type: "POST",
                    beforeSend: function () {
                        $('#loading').show();
                    },
                    complete: function () {
                        setTimeout(function () { $("#loading").hide(); }, 2000);
                    },
                    success: function (response) {
                        if (response != '') {
                            alert(response);
                        }
                    }
                });
            })
        })

    </script>

